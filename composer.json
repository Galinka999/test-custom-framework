{
    "require": {
        "symfony/dotenv": "5.4",
        "nikic/fast-route": "1.3",
        "illuminate/database": "9.50.1",
        "phpunit/phpunit": "9.6.8",
        "symfony/http-client": "^6.2"
    },
    "require-dev": {
        "symfony/var-dumper": "5.2.11"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Engine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "php": "8.0"
    }
}
